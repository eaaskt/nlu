%[scadeIntensitateLumina]('training': '1', 'testing': '30')
    ~[greet?] ~[action] ~[lumina] ~[in] @[loc] ~[la] @[nivel#relativ]
    ~[greet?] ~[action] ~[lumina] ~[in] @[loc] ~[la] @[nivel#absolut]
    ~[greet?] ~[action] ~[lumina] ~[in] @[loc]
    ~[greet?] ~[actionR] ~[luminaR] ~[in] @[loc]
    ~[greet?] ~[actionR] ~[luminaR]
    ~[greet?] ~[action] ~[lumina]

@[loc]
    ~[camara]
    ~[dormitor]
    ~[pivnita]
    ~[camera de zi]

@[nivel#relativ]
    ~[sfert]

@[nivel#absolut]
    ~[3]
    ~[4]

~[greet]
    Pepper
    Salut,

~[action]
    redu intensitatea
    as vrea sa reduci intensitatea
    poti sa reduci intensitatea
    diminueaza intensitatea
    fa mai mica intensitatea

~[lumina]
    becurilor

~[actionR]
    redu
    fa mai mica

~[luminaR]
    lumina
